		<%= link_to "Back to " + @series.title, {:action => 'index', :series_id => @series}, :class => 'back'%>
	</div>
	<h1><%= @page_title  %></h1>
</div>
<div class="breaker">&nbsp;</div>

<!--start edit form-->
	<%= form_for :message, :url => {:action => 'update', :id => @message, :series_id => @series}, :html => {:mulitpart => true} do |f| %>
