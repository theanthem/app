<% @page_title = 'New News' %>
<% @page_class = 'manage form news' %>

<div id="pageTitle">
	<div class="controller_navigation">
		<%= link_to "Back to News Posts", {:action => 'index'}, :class => 'back'%>
	</div>
	<h1><%= @page_title %></h1>
</div>
<div class="breaker">&nbsp;</div>

<%= form_for :news, :url => {:action => 'create'}, :html => {:multipart => true} do |f| %>
	  <%= render :partial => 'form', :locals => {:f => f}  %>
<% end %>




