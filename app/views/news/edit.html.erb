<% @page_title = 'Update: ' + @news.title %>
<% @page_class = 'manage form news' %>

<div id="pageTitle">
	<div class="controller_navigation">
		<%= link_to "Back to News Posts", {:action => 'index'}, :class => 'back'%>
	</div>
	<h1><%= @page_title %></h1>
</div>

<%= form_for :news, :url => {:action => 'update', :id => @news.id}, :html => {:multipart => true} do |f| %>
  	  <%= render :partial => 'form', :locals => {:f => f}  %>
<% end %>





