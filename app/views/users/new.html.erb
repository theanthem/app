<% @page_title = 'New User'%>
<% @page_class = 'manage form user'%>

<div id="pageTitle" class="noctrlnav">
	<h1><%= @page_title %></h1>
</div>
<div style="margin-top:0!important" class="userlist">
	<div class="showForm">
			<% @page_title = 'New User' %>
			<%= form_for :user, :url => {:action => 'create'}, :html => { :multipart => true } do |f| %>
			<%= render :partial => 'form', :locals => {:f => f}  %>
		<% end %>
	</div>
	<div class="userActivity">
		<h3>Adding a new user</h3>
		<p>Adding a new user </p>
	</div>
	<br />
	<%= link_to('Cancel', :action => 'index') %>
</div>